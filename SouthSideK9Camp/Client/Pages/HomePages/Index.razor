    <MudButton OnClick="@RegisterMembership" Variant="Variant.Filled" Color="Color.Primary">Register Membership</MudButton>
    
</MudContainer>

@code
{
    private void RegisterMembership()
    {
        NavigationManager.NavigateTo("/membership-registration");
